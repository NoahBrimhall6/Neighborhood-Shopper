<div class="container">
  <div class="row">
    <div class="col">
      <h2 class="border-bottom border-secondary py-2 mb-3">Searching {{zip}}</h2>
    </div>
  </div>

  <div class="row">

    {{#each cards as |card|}}
    <div class="col-12 col-sm-6 col-lg-4 col-xxl-3">
      <div class="card">
        <img src="{{card.main_image}}" class="card-img-top" alt="{{card.title}}">
        <div class="card-body">
          <h5 class="card-title">{{card.title}}</h5>
          <p class="card-text">{{card.description}}</p>
          <a href="#" class="btn btn-primary cardBtn"  data-id="{{card.id}}">Go to page</a>
        </div>
      </div>
    </div>
    {{/each}}

  </div>
</div>
<script src="/scripts/search.js"></script>